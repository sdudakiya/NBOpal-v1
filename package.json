    "express": "^4.18.3",
    "express-async-handler": "^1.2.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.22.3",
    "tsx": "^4.7.1",
    "vite-plugin-pwa": "^0.19.2",
